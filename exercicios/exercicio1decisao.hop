uses exercicio8;
dec adicao : num # num -> num;
dec par : num -> truval;
dec valor : num -> num;
dec min : num # num -> num;

--- adicao (0, y) <= y;
--- adicao (x, 0) <= x;
--- adicao (0, 0) <= 9999;
--- adicao (x, y) <= x + y;

--- par n <= if n mod 2 = 0 then true else false;

--- valor 0 <= 0;
--- valor 1 <= 2;
--- valor n <= if n > 1 and n < 9 then n * 5 else n / 5;

--- min (x, y) <= if x > y then x else y;
